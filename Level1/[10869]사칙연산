import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
    
		int A, B;
		
		Scanner An = new Scanner(System.in);
	
		A = An.nextInt();
		B = An.nextInt();
		
		System.out.println(A+B);
		System.out.println(A-B);
		System.out.println(A*B);
		System.out.println(A/B);
		System.out.println(A%B);
		
	}
}
